<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SingleBrowserSuite" parallel="none">

	<test name="Run All Tests in Package m1">
		<parameter name="browser" value="chrome" />

		<packages>
			<package name="m1" />
		</packages>
	</test>

	<listeners>
		<listener class-name="listeners.ExtentListener" />
	</listeners>

</suite>
